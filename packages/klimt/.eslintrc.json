{
  "extends": [
    "next",
    "next/core-web-vitals",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "plugins": [
    "tailwindcss",
    "testing-library"
  ],  
  "rules": {
    "tailwindcss/no-custom-classname": ["warn", {
      "whitelist": [ "white-opacity", "text-shadow", "h-footer", "h-map"]
    }]
  }
}