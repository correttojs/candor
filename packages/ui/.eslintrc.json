{
  "root": true,
  "extends": [  
    "plugin:storybook/recommended",  
    "plugin:@typescript-eslint/recommended", 
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint", 
    "tailwindcss",
    "testing-library"
  ],  
  "parser": "@typescript-eslint/parser", 
  
  "rules": { 
    "tailwindcss/no-custom-classname": ["off"]
  }
}